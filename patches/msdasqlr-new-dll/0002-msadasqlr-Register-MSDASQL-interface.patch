+#pragma makedep register
+
+[
+    progid("MSDASQL.1"),
+    vi_progid("MSDASQL"),
+    threading(both),
+    uuid(c8b522cb-5cf3-11ce-ade5-00aa0044773d)
+]
+coclass MSDASQL { }
diff --git a/dlls/msdasqlr/msdasqlr.rc b/dlls/msdasqlr/msdasqlr.rc
new file mode 100644
index 0000000..20f557c
--- /dev/null
+++ b/dlls/msdasqlr/msdasqlr.rc
@@ -0,0 +1,20 @@
