+
+/* @makedep: msdasqlr.rgs */
+2 WINE_REGISTRY msdasqlr.rgs
diff --git a/dlls/msdasqlr/msdasqlr.rgs b/dlls/msdasqlr/msdasqlr.rgs
new file mode 100644
index 0000000..b5ac427
--- /dev/null
+++ b/dlls/msdasqlr/msdasqlr.rgs
@@ -0,0 +1,11 @@
+HKCR
+{
+	NoRemove CLSID
+	{
+		NoRemove {c8b522cb-5cf3-11ce-ade5-00aa0044773d} = s 'MSDASQL'
+		{
+			val 'OLEDB_SERVICES' = d 4294967295
+			'OLE DB Provider' = s 'WINE OLE DB Provider for ODBC Drivers'
+		}
+	}
+}
