+typedef struct fw_policy2
+{
+    INetFwPolicy2 INetFwPolicy2_iface;
+    LONG refs;
+} fw_policy2;
+
+static inline fw_policy2 *impl_from_INetFwPolicy2( INetFwPolicy2 *iface )
+{
+    return CONTAINING_RECORD(iface, fw_policy2, INetFwPolicy2_iface);
+}
